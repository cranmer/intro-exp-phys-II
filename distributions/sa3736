import numpy as np
from base_distribution import BaseDistribution

class Dist_sa3736(BaseDistribution):
	def __init__(self):
		self.f_max = 2
		self.x_min = 1
		self.x_max = -1


	def pdf(self, x):
		"""This is your PDF"""
		return np.((x**8)+(4*x))

	def mean(self):
		"""This is the mean of the PDF"""
		return 8./3.

	def std(self):
		"""This is the standard deviation of the pdf"""
		return 8./3.


	
